
<%= link_to 'Add Project', new_project_path, class: 'btn btn-primary mb-4' %>

<div>
    My Projects:
    <% @projects.each do |project| %>
        <div>
            <h4> <%= link_to project.name, project %> </h4>
            <p> <%= project.description %> </p>
            <%= image_tag project.project_img, size: '250', alt: project.name %>
            <%= link_to "Check it out!", project.link, target: "_blank" %>
        </div>
    <% end %>
</div>